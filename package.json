{
  "name": "voicebridge-speech-app",
  "version": "1.0.0",
  "description": "AI-powered voice accessibility platform specializing in atypical speech recognition and clarification to improve communication clarity for individuals with speech disabilities",
  "main": "index.js",
  "scripts": {
    "start": "react-scripts start",
    "lint": "eslint . --ext .js,.jsx --no-error-on-unmatched-pattern",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhcarrigan-mentorship/matanat-khalilova.git"
  },
  "keywords": [
    "AI",
    "Atypical Speech Recognition",
    "AAC",
    "Accessibility",
    "Dysarthria Support",
    "Speech Disabilities",
    "Voice Clarity Enhancement",
    "Neurodiversity",
    "Inclusive Communication",
    "Assistive Technology"
  ],
  "author": "Matanat Khalilova",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "^3.2.5",
    "typescript": "4.9.5",
    "yaml": "2.8.2"
  },
  "bugs": {
    "url": "https://github.com/nhcarrigan-mentorship/matanat-khalilova/issues"
  },
  "homepage": "https://github.com/nhcarrigan-mentorship/matanat-khalilova#readme",
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-scripts": "^5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  }
}
